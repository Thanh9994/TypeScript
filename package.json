{
  "name": "products-app",
  "private": true,
  "scripts": {
    "dev:server": "pnpm --filter backdb run dev",
    "dev:client": "pnpm --filter clientapp run dev",
    "start": "concurrently -k -n DB,APP -c white,magenta \"pnpm run dev:server\" \"pnpm run dev:client\""
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
